    "dotenv": "^16.0.1",
    "mongoose": "^6.4.0",
    "node-fetch": "^3.2.6",
    "request": "^2.88.2",
    "standard": "^17.0.0"
  },
  "devDependencies": {
    "mocha": "^10.0.0",
    "standardx": "^7.0.0"
  },
  "engines": {
    "node": "16.9.1"
  }
