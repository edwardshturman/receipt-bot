{
  "name": "receipt-bot",
  "author": "Edward Shturman",
  "bugs": "https://github.com/edwardshturman/receipt-bot/issues",
  "homepage": "https://github.com/edwardshturman/receipt-bot",
  "version": "0.1.5",
  "license": "UNLICENSED",
  "description": "Simplify expense sharing and tracking with friends",
  "type": "module",
  "repository": {
    "type": "git",
